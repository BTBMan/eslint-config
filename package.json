{
  "name": "@btbman/eslint-config-monorepo",
  "version": "0.0.3",
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "author": "BTBMan",
  "license": "MIT",
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "lint": "eslint .",
    "release": "bumpp -r && pnpm -r publish"
  },
  "devDependencies": {
    "@btbman/eslint-config": "workspace:*",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "eslint-define-config": "^1.20.0",
    "typescript": "^5.0.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint",
        "typescript",
        "prettier"
      ],
      "allowedVersions": {}
    }
  }
}
